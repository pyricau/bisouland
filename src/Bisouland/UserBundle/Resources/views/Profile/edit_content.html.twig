{{ form_start(form, {
    'action': path('fos_user_profile_edit')
}) }}
    <fieldset>
        {{ form_errors(form) }}

        {{ form_row(form.email) }}
        {{ form_row(form.username) }}
        {{ form_row(form.current_password) }}

        <input type="submit" class="button" value="{{ 'profile.edit.submit'|trans({}, 'FOSUserBundle') }}" />
    </fieldset>
{{ form_end(form) }}
